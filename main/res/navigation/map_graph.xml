<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map_graph"
    app:startDestination="@id/map">

    <fragment
        android:id="@+id/map"
        tools:layout="@layout/fragment_map"
        android:name="ru.carwash.ui.main.map.MapFragment">
        <action
            android:id="@+id/action_map_to_viewCarwash"
            app:destination="@id/viewCarwash" />
    </fragment>

    <fragment
        android:id="@+id/viewCarwash"
        tools:layout="@layout/fragment_view_carwash"
        android:name="ru.carwash.ui.main.map.ViewCarwashFragment"/>

</navigation>